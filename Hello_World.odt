.globl main
.data 
carlo:  .asciiz "\nHello Carlo!!"
x: .float 4.0
y: .float 5.0
.text
main: 
    l.s $f1, x # load into f1
    l.s $f2, y # load into f2
    add.s $f6, $f1, $f2 # addition
    mov.s $f12, $f6 # for floating point I have to move inside the dedicate register
    li $v0, 2 # Print float
    syscall
    la $a0, carlo  # Load inside the address
    li $v0, 4      # Print the values
    
    syscall
    
    li $v0, 10
    syscall